.Table
{
     display: table;
}
.Title
 {
     display: table-caption;
     text-align: center;
     font-weight: bold;
     font-size: larger;
}
.Heading
 {
     display: table-row;
     font-weight: bold;
     text-align: center;
}
.Row
 {
     display: table-row;
}
.Cell
 {
     display: table-cell;
}
.white_row
{
     background:#FFFFFF;
     font-weight:normal;
     text-align:left;
 }
.light_blue_row
{
     background:#EfEfeF;
     font-weight:normal;
     text-align:left;
}

.group_name
{
    font-family: 'Londrina Shadow', cursive;
    font-size: 60px;
    color: #63a504;
}

.welcome
{
    font-family: helvetica, sans-serif;
    padding-top:10px;
    margin: 0 auto;
    max-width: 900px;
}

.float_recipes
{
    padding-bottom:20px;  
}
.Image_recipes
{
    background-color:#EfEfeF;
    border-top-left-radius:     15px;
    border-top-right-radius:    15px;
}
.Text_recipes
{
    padding-top:8px;
    padding-bottom:30px;    
    border-bottom-left-radius:     15px;
    border-bottom-right-radius:    15px;
    max-height:30px;  
    background: linear-gradient(rgba(99,200,4,.4), rgba(99,165,4,1));
    position: relative;     
    text-align: center;
    bottom: 0px;
    margin: 0px;    
}

.Text_recipes, .Image_recipes
{
    border: 1px solid black;
    width: 260px;    
}

.signup_facebook
{
    background-color: #3b579d;
    background: #2275d1;
}

.signup_google
{
    background-color: #dd381d;
    background: #dd381d;
}

.signup_connect
{
    background-color: #63a504;
    background: #63a504;
}

.login_button
{
    display: block;
    font-size: 13px;
    color: #fff;
    padding: 8px 20px;
    cursor: pointer;
    border-radius: 100px;
    white-space: nowrap;
    line-height: 20px;
    font-weight: 700;
    -webkit-font-smoothing: antialiased;
}

.login_form
{
    padding-top: 20px;
    width: 300px;
    margin: 0 auto;
    text-align: center;
}

input[type=submit]
{
    background-color: #63a504;
    background: #63a504;
    display: block;
    font-size: 13px;
    color: #fff;
    border: 0px;
    border-radius: 100px;
    white-space: nowrap;
    line-height: 20px;
    font-weight: 700;
    width: 300px;
    padding: 10px 10px 10px 10px;
    -webkit-font-smoothing: antialiased;
}
